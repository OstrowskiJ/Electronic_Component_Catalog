<div class="containertable">
  <table class="table table-responsive">
  <thead>
    <tr>
      <th>Element</th>
      <th>Category</th>
      <th>Value</th>
      <th>Amount</th>
    </tr>
  </thead>
  <% @catalogs.each do |catalog| %>
  <tbody>
  <tr>
    <td><%= link_to catalog.element, catalog %></td>
    <td><%=catalog.category%></td>
    <td><%=catalog.value%></td>
    <td><%=catalog.amount%></td>
  </tr>
<% end %>
  </table>
<%= link_to "Add an element", new_catalog_path %>
</div>
